    "@tanem/react-nprogress": "^5.0.55",
    "@tanstack/react-query": "^5.80.10",
    "@trpc/client": "^11.4.2",
    "@trpc/server": "^11.4.2",
    "@trpc/tanstack-react-query": "^11.4.2",
    "better-auth": "^1.2.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
