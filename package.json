    "@react-router/dev": "^7.6.2",
    "@sly-cli/sly": "^2.1.1",
    "@svgr/cli": "^8.1.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.10",
    "@types/bun": "^1.2.17",
    "@types/color": "^4.2.0",
    "@types/culori": "^4.0.0",
    "@types/node": "^22.15.33",
    "@types/pluralize": "^0.0.33",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "depcheck": "^1.4.7",
    "dotenv-cli": "^8.0.0",
    "husky": "^9.1.7",
    "jsrepo": "^2.4.2",
    "lint-staged": "^16.1.2",
    "prisma": "^6.10.1",
    "shadcn": "^2.7.0",
    "tailwindcss": "^4.1.10",
    "taze": "^19.1.0",
    "turbo": "^2.5.4",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.8.3",
    "ultracite": "^5.0.18",
    "vite": "^7.0.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,jsonc,css,scss,md,mdx}": [
      "bun ultracite format"
    ]
  }
}
