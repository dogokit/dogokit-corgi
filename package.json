{
  "name": "dogokit-corgi",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "packageManager": "bun@1.2.7",
  "scripts": {
    "build": "bun db:migrate:deploy && bun db:gen:prod && react-router build",
    "dev": "bun db:migrate && react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "check": "bun biome:check",
    "write": "bun biome:format && bun biome:lint",
    "biome:check": "biome check && bun rr:check",
    "biome:format": "biome format --write .",
    "biome:lint": "biome lint --write .",
    "rr:check": "react-router typegen && tsc",
    "dep:check": "depcheck",
    "dep:update": "taze && taze major",
    "dep:update:write": "taze -w && taze major -w",
    "dep:sly": "sly add --overwrite",
    "dep:svg": "svgr --ignore-existing --typescript --jsx-runtime automatic --filename-case kebab --icon --title-prop --out-dir app/components/icons public/icons/*.svg",
    "db:up": "docker compose up -d",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:gen": "prisma generate --no-hints",
    "db:gen:prod": "prisma generate --no-engine --no-hints",
    "db:studio": "prisma studio --browser none",
    "db:reset": "prisma migrate reset --skip-seed",
    "db:seed": "prisma db seed",
    "db:prune": "bun db:reset && bun db:seed",
    "prod:db:studio": "dotenv -e .env.prod prisma studio --browser none",
    "auth:gen": "bunx @better-auth/cli generate --config app/server/better-auth.ts"
  },
  "prisma": {
    "seed": "bun prisma/seed.ts"
  },
  "dependencies": {
    "@base-ui-components/react": "^1.0.0-beta.0",
    "@conform-to/react": "^1.7.2",
    "@conform-to/zod": "^1.7.2",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@polar-sh/better-auth": "^1.0.3",
    "@polar-sh/sdk": "^0.34.2",
    "@prisma/client": "^6.10.1",
    "@prisma/extension-accelerate": "^2.0.1",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "@react-router/node": "^7.6.2",
    "@react-router/serve": "^7.6.2",
    "@shikijs/transformers": "^3.7.0",
    "@t3-oss/env-core": "^0.13.8",
    "@tanem/react-nprogress": "^5.0.55",
    "@tanstack/react-query": "^5.81.2",
    "@trpc/client": "^11.4.2",
    "@trpc/server": "^11.4.2",
    "@trpc/tanstack-react-query": "^11.4.2",
    "@vercel/react-router": "^1.1.2",
    "argon2": "^0.43.0",
    "better-auth": "^1.2.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "color": "^5.0.0",
    "country-data-list": "^1.4.1",
    "culori": "^4.0.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "flagpack-core": "^2.0.0",
    "html-react-parser": "^5.2.5",
    "input-otp": "^1.4.2",
    "isbot": "^5.1.28",
    "libphonenumber-js": "^1.12.9",
    "lucide-react": "^0.523.0",
    "motion": "^12.19.1",
    "nanoid": "^5.1.5",
    "next-themes": "^0.4.6",
    "pluralize": "^8.0.0",
    "qrcode": "^1.5.4",
    "radix-ui": "^1.4.2",
    "react": "^19.1.0",
    "react-circle-flags": "^0.0.23",
    "react-day-picker": "^9.7.0",
    "react-dom": "^19.1.0",
    "react-fast-marquee": "^1.6.5",
    "react-flagpack": "^2.0.6",
    "react-hook-form": "^7.58.1",
    "react-medium-image-zoom": "^5.2.14",
    "react-resizable-panels": "^3.0.3",
    "react-router": "^7.6.2",
    "recharts": "^2.15.4",
    "remix-themes": "^2.0.4",
    "shiki": "^3.7.0",
    "slugify": "^1.6.6",
    "sonner": "^2.0.5",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.3.1",
    "ts-key-enum": "^3.0.13",
    "ts-plugin-sort-import-suggestions": "^1.0.4",
    "tunnel-rat": "^0.1.2",
    "vaul": "^1.1.2",
