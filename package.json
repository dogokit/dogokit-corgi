{
  "name": "dogokit-corgi",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun db:migrate:deploy && bun db:gen:prod && react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "check": "bun b:check && bun rr:check && bun dep:check",
    "rr:check": "react-router typegen && tsc",
    "b:check": "biome check",
    "b:format": "biome format --write .",
    "b:lint": "biome lint --write .",
    "dep:check": "depcheck",
    "dep:update": "taze",
    "db:up": "docker compose up -d",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:gen": "prisma generate --no-hints",
    "db:gen:prod": "prisma generate --no-hints",
    "db:studio": "prisma studio --browser none",
    "db:reset": "prisma migrate reset --skip-seed",
    "db:seed": "prisma db seed",
    "db:prune": "bun db:reset && bun db:migrate && bun db:seed"
  },
  "prisma": {
    "seed": "bun prisma/seed.ts"
  },
  "dependencies": {
